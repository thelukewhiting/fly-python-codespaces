{
    "name": "Fly.io Python",
    "dockerFile": "Dockerfile",
    "context": "..",
    "remoteUser": "vscode",
    "settings": {
        "python.defaultInterpreterPath": "/workspaces/fly-python-codespaces/.venv/bin/python",
        "python.pythonPath": "/workspaces/fly-python-codespaces/.venv/bin/python",
        "python.linting.enabled": true,
        "python.linting.pylintEnabled": true,
        "python.formatting.autopep8Path": "/workspaces/fly-python-codespaces/.venv/bin/autopep8",
        "python.formatting.blackPath": "/workspaces/fly-python-codespaces/.venv/bin/black",
        "python.formatting.yapfPath": "/workspaces/fly-python-codespaces/.venv/bin/yapf",
        "python.linting.banditPath": "/workspaces/fly-python-codespaces/.venv/bin/bandit",
        "python.linting.flake8Path": "/workspaces/fly-python-codespaces/.venv/bin/flake8",
        "python.linting.mypyPath": "/workspaces/fly-python-codespaces/.venv/bin/mypy",
        "python.linting.pycodestylePath": "/workspaces/fly-python-codespaces/.venv/bin/pycodestyle",
        "python.linting.pydocstylePath": "/workspaces/fly-python-codespaces/.venv/bin/pydocstyle"
    },
    "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
    ],
    "postCreateCommand": "poetry install && . /workspaces/fly-python-codespaces/.venv/bin/activate"
}